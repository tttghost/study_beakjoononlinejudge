   "source": [
    "N,M=map(int,input().split())\n",
    "li=[]\n",
    "result=[]\n",
    "\n",
    "def back_tracking():\n",
    "    if len(li)==M:\n",
    "        result.append(' '.join(li))\n",
    "        return\n",
    "    for i in range(1, N+1):\n",
    "        li.append(str(i))\n",
    "        back_tracking()\n",
    "        li.pop()\n",
    " \n",
    "back_tracking()\n",
    "\n",
    "print('\\n'.join(result))"
   ]
  }
 ],
