    "def fac(n):\n",
    "    if n>1:\n",
    "        return n*fac(n-1)\n",
    "    else:\n",
    "        return 1\n",
    "k=int(input())\n",
    "for _ in range(k):\n",
    "    N,K=map(int,input().split())\n",
    "    a=max(N,K)\n",
    "    b=min(N,K)\n",
    "    result=fac(a)//(fac(b)*fac(a-b))\n",
    "    print(result)"
   ]
